	"enhancement": [
		{
			"title": "Set User Info",
			"name": "setUserInfo",
			"description": "Triggers when we set username in the launchpad",
			"param": "",
			"sort": 32,
			"group": "Common",
			"content": [
				"==OBJECT STRING==",
				"",
				"// User icon + initials - Set User Info",
				"function getInitialsTextForAvatar() {",
				"",
				"    //Get the user name",
				"    let name = AppCache.userInfo.name;",
				"    let arrInitials = name.split(/,\\s|\\s/);",
				"    let initials = '';",
				"",
				"    //Get the number of names",
				"    var numberOfNames = arrInitials.length;",
				"",
				"    //5plit the names and subtract the first initial of the first and last name",
				"    initials += arrInitials[0].substring(0, 1);",
				"    initials += arrInitials[numberOfNames - 1].substring(0, 1);",
				"",
				"    return initials;",
				"",
				"}",
				"",
				"",
				"//Set the initials to the icon ",
				"",
				"function renderInitialsAvatar() {",
				"",
				"    let initials = getInitialsTextForAvatar();",
				"    if (initials !== '' && initials !== null) {",
				"        //top menu",
				"        AppCacheShellUser.setIcon('');",
				"        AppCacheShellUser.setText(initials);",
				"        AppCacheShellUser.addStyleClass(\"InitialsAvatar\");",
				"",
				"        //inside launchpad settings popup ",
				"        launchpadSettingsBtn.setIcon('');",
				"        launchpadSettingsBtn.setText(initials);",
				"        launchpadSettingsBtn.addStyleClass(\"InitialsAvatar\");",
				"    }",
				"}",
				"renderInitialsAvatar();",
				"",
				""
			],
			"active": false,
			"disabled": false
		}
	],
